{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "index.js"
    },
    {
      "src": "/search",
      "dest": "index.js"
    },
    {
      "src": "/ongoing",
      "dest": "index.js"
    },
    {
      "src": "/completed",
      "dest": "index.js"
    },
    {
      "src": "/anime/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/episode/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/cache/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}